{
    "name": "tightenco/quicksand",
    "description": "Easily schedule regular cleanup of old soft-deleted Eloquent data.",
    "authors": [
        {
            "name": "Benson Lee",
            "email": "benson@tighten.co"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "illuminate/database": "5.*",
        "nesbot/carbon": "^1.21",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Quicksand\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\Quicksand\\QuicksandServiceProvider"
            ]
        }
    }
}
